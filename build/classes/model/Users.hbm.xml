<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
	<class name="Users" table="users">
		<id name="user_id" column="user_id" type="string">
			
		</id>
		<property name="user_name" column="user_name" type="string"></property>
		<property name="state" column="state" type="int"></property>
		<property name="user_dept" column="user_dept" type="string"></property>
		<property name="user_major" column="user_major" type="string"></property>
	</class>
</hibernate-mapping>