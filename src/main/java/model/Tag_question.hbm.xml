<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
	<class name="Tag_question" table="tag_question">
		<composite-id>
			<key-property name="tag_id" column="tag_id" type="int" ></key-property>
			<key-property name="question_id" column="question_id" type="int"></key-property>
		</composite-id>
	</class>
</hibernate-mapping>